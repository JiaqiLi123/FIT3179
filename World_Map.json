{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "width": 500,
  "height": 300,
  "background":"lightblue",
  "title": "Global volcano distribution",
  "projection": {"type": "equalEarth"},
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/JiaqiLi123/FIT379/master/ne_110m.json",
        "format": {"type": "topojson", "feature": "ne_110m_admin_0_countries"}
      },
      "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/JiaqiLi123/FIT379/master/Cleaned_data.csv"
      },
      "mark": {"type": "circle", "tooltip": {"content": "data"},"color":"red"},
      "encoding": {
        "longitude": {"field": "Longitude", "type": "quantitative"},
        "latitude": {"field": "Latitude", "type": "quantitative"},
        "tooltip": [
      {"field": "Name", "type": "nominal","title":"Volcanoes name"},
      {"field": "Longitude", "aggregate": "count", "type": "nominal","title":"Longitude"},
      {"field": "Latitude", "aggregate": "count", "type": "nominal","title":"Latitude"}
    ]
      }
    }
  ],
  "config": {}
}
