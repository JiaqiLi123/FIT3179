{
  "$schema": "https://vega.github.io/schema/vega-lite/v4.json",
  "width": 650,
  "height": 400,
  "title": "Earthquakes of magnitude 4 or large between 4 Oct 2020 and 10 Oct 2020",
  "projection": {"type": "equalEarth"},
  "layer": [
    {
      "data": {
        "url": "https://raw.githubusercontent.com/JiaqiLi123/3179/main/ne_110m.json",
        "format": {"type": "topojson", "feature": "ne_110m_admin_0_countries"}
      },
      "mark": {"type": "geoshape", "fill": "lightgray", "stroke": "white"}
    },
    {
      "data": {
        "url": "https://raw.githubusercontent.com/JiaqiLi123/3179/main/database.csv"
      },
      "mark": {"type": "circle", "tooltip": {"content": "data"}},
      "encoding": {
        "longitude": {"field": "Longitude", "type": "quantitative"},
        "latitude": {"field": "Latitude", "type": "quantitative"},
        "size": {
          "field": "Elevation (Meters)",
          "type": "quantitative",
          "scale": {"domain": [4, 8000]}
        },
        "color": {
          "field": "Elevation (Meters)",
          "type": "quantitative",
          "scale": {"scheme": "reds"}
        }
      }
    }
  ],
  "config": {}
}
